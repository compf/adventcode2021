import os
important={".go",".java",".rs",".js",".ts",".cs",".cpp",".c",".py"}
counter=dict()
for p in os.walk("."):
    for f in p[2]:
        print(f)
        ext="."+f.split(".")[-1]
        if ext not in important  or "Assembly" in f:
            continue
        if ext not in counter:
            counter[ext]=0
        counter[ext]+=1
for c in counter:
    print(c,counter[c])